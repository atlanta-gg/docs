{
  "$schema": "https://mintlify.com/docs.json",
  "name": "atlanta.gg",
  "theme": "maple",
  "colors":
  {
    "primary": "#AF3264",
    "light": "#AF3264",
    "dark": "#AF3264"
  },
  "styling":
  {
    "eyebrows": "breadcrumbs"
  },
  "favicon": "/assets/images/icon.ico",
  "logo":
  {
    "light": "/assets/images/icon.png",
    "dark": "/assets/images/icon.png",
    "href": "https://atl.lat/docs"
  },
  "icons":
  {
    "library": "lucide"
  },
  "banner":
  {
    "content": "If you encounter any issues within the loader or software itself, please check our dedicated pages for Errors, Resources, or Tips for solutions."
  },
  "navigation":
  {
    "groups":
    [
      {
        "group": "Documentation",
        "pages":
        [
          "docs/index"
        ],
        "hidden": true
      },
      {
        "group": "Guides",
        "pages":
        [
          "guides/getting-started",
          "guides/navigation",
          "guides/errors",
          "guides/resources",
          "guides/tips"
        ]
      },
      {
        "group": "Roblox",
        "pages":
        [
          {
            "group": "Features",
            "expanded": true,
            "icon": "settings-2",
            "pages":
            [
              "roblox/features/getting-started"
            ]
          },
          {
            "group": "Lua",
            "expanded": true,
            "icon": "braces",
            "pages":
            [
              "roblox/lua/constants",
              "roblox/lua/enums",
              "roblox/lua/types",
              "roblox/lua/cheat",
              "roblox/lua/entry",
              "roblox/lua/event",
              "roblox/lua/menu",
              "roblox/lua/draw",
              "roblox/lua/memory",
              "roblox/lua/crypto",
              "roblox/lua/input",
              "roblox/lua/utils"
            ]
          }
        ]
      }
    ]
  },
  "navbar":
  {
    "primary":
    {
      "type": "button",
      "label": "Website",
      "href": "https://atlanta.gg"
    }
  },
  "contextual":
  {
    "options":
    [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
    ]
  },
  "footer":
  {
    "socials":
    {
      "github": "https://github.com/atlanta-gg",
      "website": "https://atlanta.gg"
    }
  },
  "errors":
  {
    "404":
    {
      "redirect": false,
      "title": "Invalid Page",
      "description": "Page could not be found."
    }
  }
}